{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build Solution",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "Build AppTest",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/CliToolkit.AppTest"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "Clean Build Artifacts",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        },
        {
            "label": "Restore Packages",
            "command": "dotnet",
            "type": "process",
            "args": [
                "restore",
                "${workspaceFolder}"
            ],
            "problemMatcher": "$msCompile",
            "group": "build"
        }
    ]
}