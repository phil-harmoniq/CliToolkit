<Project>

  <PropertyGroup>
    <MajorVersion>0</MajorVersion>
    <MinorVersion>3</MinorVersion>
  </PropertyGroup>

  <PropertyGroup>
    <Authors>Phil Hawkins</Authors>
    <Copyright>MIT</Copyright>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
    <Description>Build powerful command-line appls with configuration and DI abstractions out-of-the-box.</Description>
    <PackageIconUrl></PackageIconUrl>
    <PackageTags>cli;terminal;command;configuration;di</PackageTags>
    <LicenseUrl>https://raw.githubusercontent.com/phil-harmoniq/CliToolkit/develop/LICENSE.md</LicenseUrl>
    <PackageProjectUrl>https://phil.harmoniq.dev/CliToolkit</PackageProjectUrl>
    <RepositoryUrl>https://github.com/phil-harmoniq/CliToolkit</RepositoryUrl>
    <IsPackable>false</IsPackable>
  </PropertyGroup>

  <PropertyGroup>
    <VersionPrefix>$(MajorVersion).$(MinorVersion)</VersionPrefix>
    <VersionPrefix Condition=" '$(BUILD_BUILDID)' != '' ">$(VersionPrefix).$(BUILD_BUILDID)</VersionPrefix>
    <VersionSuffix Condition=" '$(BUILD_SOURCEBRANCHNAME)' != 'master' ">dev</VersionSuffix>
  </PropertyGroup>

</Project>